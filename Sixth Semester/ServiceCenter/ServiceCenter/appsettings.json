{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "RabbitMQ": {
    "Host": "localhost",
    "QueueName": "orders_queue"
  },

  "Jaeger": {
    "AgentHost": "localhost",
    "AgentPort": 6831
  },
  "OpenTelemetry": {
    "ServiceName": "MyService"
  }
}

